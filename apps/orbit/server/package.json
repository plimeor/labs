{
  "name": "@orbit/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun src/index.ts",
    "type-check": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.33",
    "@elysiajs/cors": "^1.0.0",
    "@elysiajs/swagger": "^1.0.0",
    "@orbit/shared": "workspace:*",
    "@plimeor-labs/logger": "workspace:*",
    "cron-parser": "^5.5.0",
    "date-fns": "^4.1.0",
    "elysia": "^1.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@plimeor-labs/tsconfig": "workspace:*",
    "@types/bun": "latest",
    "typescript": "^5"
  }
}
